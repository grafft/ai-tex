	keywords = {goalres},
	mendeley-groups = {Cogntive approach/Cognitive modelling/Behaviour},
	pages = {105--122},
	publisher = {Elsevier B.V.},
	title = {{Goal reasoning as a general form of metacognition in BICA}},
	url = {http://dx.doi.org/10.1016/j.bica.2014.07.003},
	volume = {9},
	year = {2014}
}

