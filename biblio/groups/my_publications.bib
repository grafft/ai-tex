issn = {1877-0509},
journal = {Procedia Computer Science},
keywords = {16-11-00048,behavior planning,computer cognitive modeling,frccsc,map,myconf,path planning,planner,scopus,sign world model,theory of activity,wos{\_}core},
mendeley-tags = {16-11-00048,frccsc,myconf,scopus,wos{\_}core},
pages = {115--124},
title = {{Psychologically Inspired Planning Method for Smart Relocation Task}},
url = {http://www.sciencedirect.com/science/article/pii/S1877050916316702},
volume = {88},
year = {2016}
}
@article{Panov2015a,
