@article{Chiang2014,
abstract = {Alterations of DNA methylation occur during the course of both stem cell development and tumorigenesis. We present a novel strategy that can be used to stratify glioblastoma multiforme (GBM) patients through the epigenetic states of genes associated with human embryonic stem cell (hESC) identity in order to 1) assess linkages between the methylation signatures of these stem cell genes and survival of GBM patients, and 2) delineate putative mechanisms leading to poor prognosis in some patient subgroups. A DNA methylation signature was established for stratifying GBM patients into several hESC methylator subgroups. The hESC methylator-negative phenotype has demonstrated poor survival and upregulation of glioma stem cell (GSC) markers, and is enriched in one of the previously defined transcriptomic phenotypes-the mesenchymal phenotype. We further identified a refined signature of 36 genes as the gene panel, including SOX2, POU3F2, FGFR2, GAP43, NTRK2, NTRK3, and NKX2-2, which are highly enriched in the nervous system. Both signatures outperformed the O6-methylguanine-DNA methyltransferase (MGMT) methylation test in predicting patient's outcome. These findings were also validated through an independent dataset of patients. Furthermore, through statistical analyses, both signatures were examined significantly. Hypomethylation of hESC-associated genes predicted poorer clinical outcome in GBM, supporting the idea that epigenetic activation of stem cell genes contributes to GBM aggression. The gene panel presented herein may be developed into clinical assays for patient stratification and future personalized medicine interventions.},
author = {Chiang, Jung-Hsien and Cheng, Wan-Shu and Hood, Leroy and Tian, Qiang},
doi = {10.1089/omi.2013.0084},
file = {:C$\backslash$:/Users/sanek/Documents/Mendeley Desktop/Chiang et al/2014/An epigenetic biomarker panel for glioblastoma multiforme personalized medicine through DNA methylation analysis of human embryonic stem.pdf:pdf},
issn = {1557-8100},
journal = {Omics : a journal of integrative biology},
keywords = {Area Under Curve,Biological,Biological: genetics,Brain Neoplasms,Brain Neoplasms: diagnosis,Brain Neoplasms: genetics,Brain Neoplasms: mortality,DNA Methylation,Embryonic Stem Cells,Embryonic Stem Cells: physiology,Epigenesis,Gene Expression Regulation,Genetic,Genetic Markers,Glioblastoma,Glioblastoma: diagnosis,Glioblastoma: genetics,Glioblastoma: mortality,Humans,Individualized Medicine,Kaplan-Meier Estimate,Neoplastic,Neoplastic Stem Cells,Neoplastic Stem Cells: physiology,Prognosis,ROC Curve,Tumor Markers},
number = {5},
pages = {310--323},
pmid = {24601786},
title = {{An epigenetic biomarker panel for glioblastoma multiforme personalized medicine through DNA methylation analysis of human embryonic stem cell-like signature}},
