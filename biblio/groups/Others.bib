isbn = {978-972-757-716-3},
issn = {09254560},
journal = {Www.Bitcoin.Org},
pages = {9},
pmid = {14533183},
title = {{Bitcoin: A Peer-to-Peer Electronic Cash System}},
url = {https://bitcoin.org/bitcoin.pdf},
year = {2008}
}
