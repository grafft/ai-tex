file = {:C$\backslash$:/Users/panov/Documents/Mendeley Desktop/Perspectives on Psychological Science/Evans, Stanovich/Evans, Stanovich - 2013 - Dual-process theories of higher cognition Advancing the debate.pdf:pdf},
