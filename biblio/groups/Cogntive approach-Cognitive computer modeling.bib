keywords = {16-11-00048,osipov},
mendeley-tags = {16-11-00048,osipov},
pages = {215--230},
publisher = {Springer},
title = {{Sign-Based Representation and World Model of Actor}},
url = {http://link.springer.com/10.1007/978-3-319-78437-3{\_}9},
year = {2018}
}
