	keywords = {16-11-00048,behavior planning,frccsc,group of robots,hse,multiagent planning,myconf,robots,scopus,sign,sign based world model,task planning},
	mendeley-groups = {my{\_}publications},
	mendeley-tags = {16-11-00048,frccsc,hse,myconf,scopus},
	pages = {83--94},
	publisher = {Springer},
	series = {Lecture Notes in Computer Science},
	title = {{Synthesis of the Behavior Plan for Group of Robots with Sign Based World Model}},
	url = {https://link.springer.com/chapter/10.1007/978-3-319-66471-2{\_}10},
	year = {2017}
