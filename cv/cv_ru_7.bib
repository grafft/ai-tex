	journal = {Workshop on Planning and Learning (PAL-18)},
	keywords = {16-11-00048,HAM,Hierarchical Reinforcement Learning,Reinforcement Learning,frccsc,hierarchy formation,myconf,skill discovery},
	mendeley-groups = {my{\_}publications},
